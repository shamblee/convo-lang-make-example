    const deckId = id || DEFAULT_DECK_ID
    const list = asArray(decks)
    return list.find((d) => d.id === deckId) || list[0]
  }, [decks, id])

  const [workingName, setWorkingName] = useState<string>("")
  const [workingCards, setWorkingCards] = useState<string[]>([])

